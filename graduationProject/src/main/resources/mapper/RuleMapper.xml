<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rock.mapper.RuleMapper">
    <select id="getRuleById" resultType="com.rock.entity.Rule">
        SELECT rid,cypher FROM Rule WHERE rid = #{rid}
    </select>

</mapper>